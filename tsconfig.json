{
	"compilerOptions": {
	  "target": "es5",
	  "module": "commonjs",
	  "jsx": "react",
	  "strict": false,
	  "noImplicitAny": true,
	  "noUnusedLocals": true,
	  "noUnusedParameters": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "sourceMap": true,
	  "outDir": "./dist",
	  "rootDir": "./src"
	},
	"include": ["./src/**/*.ts", "./src/**/*.tsx", "src/index.tsx"],
	"exclude": ["node_modules"]
      }